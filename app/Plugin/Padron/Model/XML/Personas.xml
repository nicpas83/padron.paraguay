<Personas>
    <search_list file="PersonasSL.php" >
        <searchfield name="prestacion" label="Prestación" presentation="AUTOCOMPLETE" classparams="{'model':'Personas.Prestacion'}" />
        <searchfield name="fecha_carga" label="Fecha" presentation="DATERANGE" />
        <searchfield name="vecino" label="Vecino" />
        <searchfield name="estado" label="Estado" presentation="ESTADOS_RECLAMOS" initialvalue="Inicial" />
        
        <table>
            <column>
                <listfield name="nombre" label="Nombre"  />
            </column>
            <column>
                <listfield name="apellido" label="Apellido" />
            </column>
            <column>
                <listfield name="cedula" label="Cedula"  />
            </column>
            <column>
                <listfield name="fecha_nacimiento" label="Fecha Nac." presentation="DATE"/>
            </column>
            <column>
                <listfield name="telefono" label="Tel Fijo" />
            </column>
            <column>
                <listfield name="celular" label="Tel Celular"  />
            </column>
            <column>
                <listfield name="email" label="Email"  />
            </column>
            <column>
                <listfield name="domicilio" label="Domicilio"  />
            </column>
            
            
        </table>
        <actions>
            <action op="A" action="add" global="true"  />
            <action op="V" action="view" />
            <action op="E" action="edit" />
        </actions>
    </search_list>
    
    <tablemaint file="PersonasMaint.php">
        <fieldset title="Datos Personales" columns="2" id="reclamo" blocks="12" >
            <field name="nombre" label="Nombre" />
            <field name="apellido" label="Apellido"  />
            <field name="cedula" label="Cedula" presetation="INT" />
            <field name="fecha_nacimiento" label="Fecha Nac" presentation="DATE" />
            <field name="telefono" label="Tel Fijo" presentation="INT" />
            <field name="celular" label="Tel Celular" presentation="INT" />
            <field name="email" label="Email"  />
        </fieldset>
        
        
        <fieldset title="Ubicación del Reclamo" id="ubicacion">
            <field name="direccion" label="Dirección" />
            <field name="tmp_provincia" label="Provincia" initialvalue="Campana, Provincia de Buenos Aires" isvisible="false" />
            <field name="tmp_pais" label="País" initialvalue="Argentina" isvisible="false" />
        </fieldset>        
        <fieldset title="Mapa" id="mapa">
            <field name="coordenadas" label="" presentation="GOOGLEMAP" classparams="{'calle_altura':'direccion', 'provincia':'tmp_provincia', 'pais':'tmp_pais'}" />
        </fieldset>

        <files title="Archivos Adjuntos" />
    </tablemaint>
    
